{
  "name": "invest",
  "version": "1.0.0",
  "description": "A fintech app",
  "main": "app.js",
  "repository": "https://github.com/Ugochidev/invest",
  "author": "Ugochidev",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/shared/server.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/config/databases/postgresql.ts --name migrations ./src/migrations",
    "migration:up": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/databases/postgresql.ts",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/databases/postgresql.ts",
    "seed": "ts-node ./src/shared/seeds/users.ts"
  },
  "private": false,
  "dependencies": {
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "ioredis": "^5.3.1",
    "mongoose": "^6.10.0",
    "mysql2": "^3.1.2",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "typeorm": "0.3.12",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.6",
    "nodemon": "^2.0.20",
    "ts-node": "10.7.0"
  }
}
